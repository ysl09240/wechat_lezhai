    <dubbo:reference id="pmsService" interface="com.lezhai365.pms.spi.system.IPmsService"  />
    <dubbo:reference id="financeCollectService" interface="com.lezhai365.pms.spi.finance.IFinanceCollectService"  />
    <dubbo:reference id="houseService" interface="com.lezhai365.pms.spi.material.IHouseService"  />

    <!-- wechat 相关服务调用 -->
    <dubbo:reference id="ownersInfomationService" interface="com.lezhai365.pms.spi.wechat.IOwnersInfomationService"  />
    <dubbo:reference id="propertyManagerService" interface="com.lezhai365.pms.spi.wechat.IPropertyManagerService"  />
    <dubbo:reference id="propertyServiceService" interface="com.lezhai365.pms.spi.wechat.IPropertyServiceService"  />
    <dubbo:reference id="userWxAuthService" interface="com.lezhai365.pms.spi.wechat.IUserWxAuthService"  />
    <dubbo:reference id="userWxService" interface="com.lezhai365.pms.spi.wechat.IUserWxService"  />
    <dubbo:reference id="mpInfoService" interface="com.lezhai365.pms.spi.wechat.IMpInfoService"  />



