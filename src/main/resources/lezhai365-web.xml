    <dubbo:reference id="SNSUserService" interface="com.lezhai365.pms.spi.sns.ISNSUserService"  />
    <dubbo:reference id="SNSUserHouseService" interface="com.lezhai365.pms.spi.sns.ISNSUserHouseService" />
    <dubbo:reference id="SNSFinanceCollectService" interface="com.lezhai365.pms.spi.sns.ISNSFinanceCollectService"  />
    <dubbo:reference id="SNSRepairService" interface="com.lezhai365.pms.spi.sns.ISNSRepairService"  />
    <dubbo:reference id="SNSComplaintService" interface="com.lezhai365.pms.spi.sns.ISNSComplaintService"  />
    <dubbo:reference id="topicService" interface="com.lezhai365.sns.spi.topic.ITopicService"  />

    <!--base-api  end-->
</beans>
