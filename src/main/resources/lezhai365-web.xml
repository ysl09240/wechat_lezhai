    <dubbo:reference id="twitterService"   interface="com.lezhai365.sns.spi.feed.ITwitterService" />
    <dubbo:reference id="messageService"   interface="com.lezhai365.base.spi.message.IMessageService" />

    <!-- 乐宅365 物业系统相关服务调用-->
    <dubbo:reference id="noticeService"   interface="com.lezhai365.pms.spi.office.INoticeService" />
    <dubbo:reference id="complaintService" interface="com.lezhai365.pms.spi.service.IComplaintService"  />
    <dubbo:reference id="repairService" interface="com.lezhai365.pms.spi.service.IRepairService"  />
    <dubbo:reference id="ownerInfoService" interface="com.lezhai365.pms.spi.customer.IOwnerInfoService"  />
    <dubbo:reference id="pmsParameterService" interface="com.lezhai365.pms.spi.system.IPmsParameterService"  />
